package homework;

public class insurance {
	public static void main(String args[]) {
		
		int age=37;
		String name="Din",health="good";
		char gender='F';
		int premium=0,policy=0;
		if(gender=='M'){
			if(age>=25 && age<=35) {
				if(health=="good") {
					premium=4000;
					policy=200000;
				}
				else if(health=="poor") {
					premium=6000;
					policy=10000;
				}
					
			}
			else {
				System.out.println("Person is not insured");
			}
		}
		else if(gender=='F') {
			if(age>=25 && age<=35) {
				premium=3000;
				policy=100000;
			}
		}
		else {
			System.out.println("Person is not insured");
		}
	}
}