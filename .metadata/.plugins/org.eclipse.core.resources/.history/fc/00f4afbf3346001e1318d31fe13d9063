package classexe;

public class switchcases {

	public static void main(String[] args) {
		char temp='a';
		switch(temp) {
		case 'A':
		case 'E':
		case 'I':
		case 'O':
		case 'U':
		case 'a':
		case 'e':
		case 'i':
		case 'o':
		case 'u':
			System.out.println("vowel");
			break;
		default:
			System.out.println("consonant");
		
			
		}
		// TODO Auto-generated method stub

	}

}
