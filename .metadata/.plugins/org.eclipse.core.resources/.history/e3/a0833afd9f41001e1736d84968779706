package classexe;

public class greater {
	
	public static void main(String args[]) {
		int a=70,b=40,c=30,temp;
		if(a>b) {
			if(b>c) {
				temp=a;
				a=c;
				c=temp;
				System.out.println(a);
				System.out.println(b);
				System.out.println(c);
			}
			else {
				temp=a;
				a=b;
				b=c;
				c=temp;
				System.out.println(a);
				System.out.println(b);
				System.out.println(c);
				}
			}
				//System.out.println(a);
				//System.out.println(b);
				//System.out.println(c);
		}
	}


