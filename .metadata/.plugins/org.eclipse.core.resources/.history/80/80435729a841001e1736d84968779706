package classexe;

public class lesser {
	
	public static void main(String args[]) {
		int a=30,b=20,c=40,temp;
		if(a>b) { //30>20
			temp=a; //temp=30
			a=b; //a=20
			b=temp;	//b=30
		}
		if(b<c) { //30>20
			temp=b; //temp=30
			b=c; //b=10
			c=temp; //c=30
		}
		if(a<c) { //30<40
			temp=c;
			c=a;
			a=temp;
		}
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}

}
