package classexe;

public class leapjoke {

	public static void main(String[] args) {
		int year=2024,firstly=0,lastly=0,k=0;
		if(year%4==0 && (year%100!=0 || year%400==0)) {
			firstly=year/100;
			lastly=year%100;
			k=firstly+lastly;
			if(k%2==0) {
				System.out.println("l");
			}
		}
		// TODO Auto-generated method stub

	}

}
