package classexe;

public class primenum {

	public static void main(String[] args) {
		int num=8,count=0;
		int i=num;
		while(i>=1) {
			if(num%i==0) {
				count++;
			}
			i--;
		}
		if(count==2) {
			System.out.println(num+"  Prime number");
		}
		else {
			System.out.println(num+ "  Not a prime number");
		}
		System.out.println(count+"  factors");
		// TODO Auto-generated method stub

	}

}
