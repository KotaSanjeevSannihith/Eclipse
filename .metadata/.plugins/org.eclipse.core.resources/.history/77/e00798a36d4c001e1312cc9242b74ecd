package classexe;

public class forfactors {

	public static void main(String[] args) {
		
		int x,num=2,count=0;
		for(x=1;x<=num/2;x++) {
			if(num%x==0 && count<=1) {
				count++;
				System.out.println(x);
			}
			
		}
		if(count<=2) {
			System.out.println("Composite Number");
		}
		else {
			System.out.println("Prime number");
		}
		// TODO Auto-generated method stub

	}

}
